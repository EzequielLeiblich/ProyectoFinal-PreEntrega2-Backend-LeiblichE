<h1>Productos: </h1>
{{#if isValid}}
<div>
  {{#each products}}
    <div style="border: 1px solid #000; border-radius: 1rem; padding: 1rem; margin-bottom: 1rem">
      <p> Title: {{this.title}} </p>
      <p> Description: {{this.description}} </p>
      <p> Price: ${{this.price}} </p>
      <p> Catego√≠a: {{this.category}} </p>
      <p> Stock: {{this.stock}} unidades </p>
    </div>
  {{/each}}
</div>
<div>
  {{#if hasPrevPage}}
    <a href={{this.prevLink}}> <<Anterior</a>
  {{/if}}
  &nbsp;
  {{this.page}}
  &nbsp;
  {{#if hasNextPage}}
    <a href={{this.nextLink}}>Siguiente >></a>
  {{/if}}
</div>
{{else}}
<h1>No hay productos para mostrar</h1>
{{/if}}